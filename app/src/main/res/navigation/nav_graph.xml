<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@+id/mapFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.pdv.go4lunch.ui.fragment.MapFragment"
        android:label="I'm Hungry!"
        tools:layout="@layout/fragment_map"></fragment>

    <fragment
        android:id="@+id/listViewFragment"
        android:name="com.pdv.go4lunch.ui.fragment.ListViewFragment"
        android:label="I'm Hungry!"
        tools:layout="@layout/fragment_list_view" />

    <fragment
        android:id="@+id/workMatesFragment"
        android:name="com.pdv.go4lunch.ui.fragment.WorkMatesFragment"
        android:label="Available workmates"
        tools:layout="@layout/fragment_work_mates" />

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.pdv.go4lunch.ui.fragment.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/yourLunchFragment"
        android:name="com.pdv.go4lunch.ui.fragment.YourLunchFragment"
        android:label="Your Lunch"
        tools:layout="@layout/fragment_your_lunch"/>
    <fragment
        android:id="@+id/logoutFragment"
        android:name="com.pdv.go4lunch.ui.fragment.LogoutFragment"
        android:label="Logout"
        tools:layout="@layout/fragment_logout" />
</navigation>